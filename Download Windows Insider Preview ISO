  0% { opacity: 1; }
  100% { opacity: 1; }
}

@-webkit-keyframes slideIn { 
  0% { left: 3%; opacity: 0; }
  100% { left: 0px; opacity: 1; }
}

@-webkit-keyframes slideIn { 
  0% { left: 3%; opacity: 0; }
  100% { left: 0px; opacity: 1; }
}

